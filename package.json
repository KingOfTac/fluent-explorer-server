{
	"name": "fluent-explorer-server",
	"version": "0.1.0",
	"packageManager": "yarn@3.1.0",
	"main": "dist/server.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/server.js",
		"build:scripts": "tsc --project ./build/tsconfig.json",
