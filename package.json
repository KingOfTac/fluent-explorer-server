		"@types/yargs": "^17",
		"@yarnpkg/fslib": "^2.6.1-rc.2",
		"@yarnpkg/libzip": "^2.2.3-rc.2",
		"typescript": "^4.4.4",
		"yargs": "^17.2.1"
	},
	"dependencies": {
		"ajv": "^8.8.2",
		"basic-auth-connect": "^1.0.0",
		"body-parser": "^1.19.0",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"mongodb": "^4.2.1"
	}
